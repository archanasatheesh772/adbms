mysql> select emp_id,emp_name from employees where dep_no=1;
ERROR 1054 (42S22): Unknown column 'dep_no' in 'where clause'
mysql> select emp_id,emp_name from employees where dep_no = 1;
ERROR 1054 (42S22): Unknown column 'dep_no' in 'where clause'
mysql> select emp_id, emp_name from employees where dep_no = 1;
ERROR 1054 (42S22): Unknown column 'dep_no' in 'where clause'
mysql> select emp_id,emp_name,designation,dept_no,salary from employees order by salary desc;
